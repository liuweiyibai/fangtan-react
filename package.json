{
  "name": "react-h5-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "antd-mobile": "^2.3.4",
    "babel-plugin-import": "^1.13.3",
    "classnames": "^2.2.6",
    "gh-pages": "^3.1.0",
    "less-loader": "^7.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "typescript": "^4.1.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "build:test": "dotenv -e .env.test react-app-rewired build",
    "analyzer": "dotenv -e .env.test react-app-rewired build",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write 'src/**/*.{js,jsx,tsx,ts,less,md,json}'",
    "gh": "gh-pages -d dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "src/**/*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "circular-dependency-plugin": "^5.2.2",
    "customize-cra": "^1.0.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.5",
    "less": "^3.13.0",
    "lint-staged": "^10.5.3",
    "lodash-webpack-plugin": "^0.11.5",
    "postcss-normalize": "^9.0.0",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.2.1",
    "react-app-rewired": "^2.1.7",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "webpack-build-notifier": "^2.1.0",
    "webpack-bundle-analyzer": "^4.2.0",
    "webpack-dashboard": "^3.2.1",
    "webpackbar": "^5.0.0-3"
  }
}
